@model IEnumerable<SMS.Models.ViewTimeTable>

<hr>
<br /><br />

<br /><br /><br />
<style>
    table {
        font-family: arial, sans-serif;
        border-collapse: collapse;
        width: 80%;
        height: 90%;
    }

    td, th {
        border: 1px solid #dddddd;
        text-align: center;
        padding: 8px;
        height:70px;
        width:50px;
    }

    tr:nth-child(even) {
        background-color: #dddddd;
    }
</style>
<table>
    <tr>
       
        <th>
            @Html.DisplayNameFor(model => model.day)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.R8to9)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.R9to10)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.R10to11)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.R11to12)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.R1to2)
        </th>
      
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
     
            @Html.DisplayFor(modelItem => item.day)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.R8to9)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.R9to10)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.R10to11)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.R11to12)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.R1to2)
        </td>
        
    </tr>
}

</table>
