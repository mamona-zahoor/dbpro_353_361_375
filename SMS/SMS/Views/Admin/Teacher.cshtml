        </header>
        <section>
            <!-- Swiper-->
            <div class="swiper-container swiper-slider swiper-slider-1" data-loop="false" data-autoplay="5000" data-simulate-touch="false">
                <div class="swiper-wrapper">
                    <div class="swiper-slide" data-slide-bg="../images/ol.jpg">
                        <div class="swiper-slide-caption context-dark">
                            <div class="container">
                                <div class="row">
                                    <div class="col-md-10 col-lg-7">
                                        <div class="box-animation">
                                            <h6 data-caption-animate="slideInLeft" data-caption-delay="150">Quality Higher Education</h6>
                                            <h2 data-caption-animate="slideInLeft" data-caption-delay="300">
                                                Any Successful Career Starts  <span class="text-italic font-weight-bold">With Good Education</span>
                                            </h2>
                                            <div class="button-block" data-caption-animate="fadeInUp" data-caption-delay="450">
                                                <a class="button button-primary-gradient"
                                                   ="#"><span>sign up for excursion</span></a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="swiper-slide" data-slide-bg="../images/v.jpeg">
                        <div class="swiper-slide-caption context-dark">
                            <div class="container">
                                <div class="row">
                                    <div class="col-md-10 col-lg-7">
                                        <div class="box-animation">
                                            <h6 data-caption-animate="slideInLeft" data-caption-delay="150">A vAriety of programs</h6>
                                            <h2 data-caption-animate="slideInLeft" data-caption-delay="300">
                                                Providing Students with  <span class="text-italic font-weight-bold">Professional Education</span>
                                            </h2>
                                            <div class="button-block" data-caption-animate="fadeInUp" data-caption-delay="450"><a class="button button-primary-gradient" href="#"><span>sign up for excursion</span></a></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="swiper-slide" data-slide-bg="../images/s.jpeg">
                        <div class="swiper-slide-caption context-dark">
                            <div class="container">
                                <div class="row">
                                    <div class="col-md-10 col-lg-7">
                                        <div class="box-animation">
                                            <h6 data-caption-animate="slideInLeft" data-caption-delay="150">Campus Community</h6>
                                            <h2 data-caption-animate="slideInLeft" data-caption-delay="300">
                                                Become a Part of Our Campus and <span class="text-italic font-weight-bold">Make Friends</span>
                                            </h2>
                                            <div class="button-block" data-caption-animate="fadeInUp" data-caption-delay="450"><a class="button button-primary-gradient" href="#"><span>sign up for excursion</span></a></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="swiper-pagination"></div>
            </div>
        </section>
        <!-- A Few Words About Me-->


    </div>
    <div class="snackbars" id="form-output-global"></div>
    <script src="~/js/core.min.js"></script>
    <script src="~/js/script.js"></script>
    <br>

    <h1 style="color  :#b18757; font-family : Calisto MT">&emsp; &emsp; Manage Teacher </h1>
    <br>
    @using (Html.BeginForm("Teacher", "Admin", FormMethod.Get))
    {
        <h3 style="font-family : Calisto MT">
            &emsp; &emsp; &emsp; &emsp;
            <input type="text" name="Id" id="Search" placeholder="Search Teacher">

            &emsp; &emsp; &emsp; &emsp;<input type="submit" class="submit-link submit" font-size="100px" value="Search" />
            &emsp; &emsp; &emsp; &emsp;
        </h3><br />
    }
    <h3 style="color  :#ECCF3E; font-family : Calisto MT">
        &emsp; &emsp;    @Html.ActionLink("Add New Teacher", "AddTeacher")
    </h3>
    <p>
        <a class="btn btn-success" href=@Url.Action("ExportTeacherReport")> Generate Report </a>
    </p><br />

    <table align='center'>

        <tr bgcolor='#F0E68C'>
            <th>
                Id
            </th>
            <th>
                Email
            </th>
            <th>
                Salary
            </th>
            <th>
                InchargeOfSection
            </th>
            <th>
                Password
            </th>
            <th>
                Salary Details
            </th>
            <th>
                Update
            </th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Salary)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.InchSec)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Password)
                </td>
                <td>
                    @Html.ActionLink("Pay Details", "Payrolls", new { id = item.Id })
                </td>
                <td>
                    @Html.ActionLink("Edit", "EditTeacher", new { id = item.Id }) |
                    @Html.ActionLink("Details", "TeacherDetails", new { id = item.Id }) |
                    @Html.ActionLink("Delete", "DeleteTeacher", new { id = item.Id })
                </td>
            </tr>
        }

    </table>
    <br><br><br><br>
</body>




