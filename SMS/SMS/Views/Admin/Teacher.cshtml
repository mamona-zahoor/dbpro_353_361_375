@model IEnumerable<SMS.Models.Teacher>
<title>Manage Teachers</title>


@{ Layout = null;}
<head>
    <meta charset="UTF-8" name="viewport" content="width-device=width, initial-scale=1" />
    <link rel="stylesheet" type="text/css" href="~/css/bootstrap.css" />
